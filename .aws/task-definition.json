{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "aceit-prod",
    "containerDefinitions": [
        {
            "name": "aceit-prod",
            "image": "433063897868.dkr.ecr.eu-north-1.amazonaws.com/kxn/aceit:prod",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "1 vCPU",
    "executionRoleArn": "arn:aws:iam::433063897868:role/aceit-ecsTaskExecutionRole-prod"
}
